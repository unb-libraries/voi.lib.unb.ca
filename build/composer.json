{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "phpstan/extension-installer": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of voi.lib.unb.ca",
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/voi.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "name": "unb-libraries/flag_search_api",
        "source": {
          "reference": "8.x-1.x",
          "type": "git",
          "url": "https://github.com/unb-libraries/flag_search_api.git"
        },
        "type": "drupal-module",
        "version": "1.x-dev"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "^3",
    "drupal/bootstrap4": "^3",
    "drupal/color": "^1", 
    "drupal/core": "^10",
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/drush_language": "^1",
    "drupal/facets": "^2",
    "drupal/flag": "^4",
    "drupal/fontawesome": "^2",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^1",
    "drupal/quickedit": "^1", 
    "drupal/redirect": "^1",
    "drupal/redirect_after_login": "^2",
    "drupal/rdf": "^2", 
    "drupal/redis": "^1",
    "drupal/samlauth": "^3",
    "drupal/search_api": "^1",
    "drupal/search_api_autocomplete": "^1",
    "drupal/search_api_solr": "^4",
    "drupal/views_data_export": "^1",
    "unb-libraries/drupal-development-libraries": "^10",
    "unb-libraries/composer-drupal-scaffold": "^10",
    "unb-libraries/flag_search_api": "^1.x-dev",
    "unb-libraries/saml_features": "dev-10.x-1.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "type": "project"
}
