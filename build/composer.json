{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of voi.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/samlauth": {
                "Ignore Role Restrictions": "/build/patches/samlauth_rolematch.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/voi.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/admin_toolbar": "2.5",
        "drupal/bootstrap4": "2.1.14",
        "drupal/core": "9.3.5",
        "drupal/drush_language": "1.0-rc4",
        "drupal/facets": "2.0.1",
        "drupal/flag": "4.0-beta3",
        "drupal/flag_search_api": "dev-1.x",
        "drupal/fontawesome": "2.19",
        "drupal/google_analytics": "2.5",
        "drupal/health_check": "1.3",
        "drupal/redirect": "1.7",
        "drupal/redirect_after_login": "2.7",
        "drupal/redis": "1.5",
        "drupal/samlauth": "3.4",
        "drupal/search_api": "1.23",
        "drupal/search_api_autocomplete": "1.6",
        "drupal/search_api_solr": "4.2.7",
        "drupal/views_data_export": "1.1",
        "unb-libraries/drupal-builder": "^9",
        "unb-libraries/saml_features": "dev-9.x-1.x"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^9"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
