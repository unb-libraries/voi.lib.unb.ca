{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of voi.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/samlauth": {
                "samlauth nocache metadata": "/build/patches/samlauth_nocache_metadata.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/voi.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/admin_toolbar": "2.4",
        "drupal/bootstrap4": "2.1.2",
        "drupal/core": "8.9.9",
        "drupal/drush_language": "1.0-rc4",
        "drupal/facets": "1.6",
        "drupal/flag": "4.0-beta2",
        "drupal/flag_search_api": "1.4",
        "drupal/fontawesome": "2.17",
        "drupal/google_analytics": "2.5",
        "drupal/health_check": "1.2",
        "drupal/migrate_plus": "4.1",
        "drupal/migrate_upgrade": "3.0",
        "drupal/migrate_source_csv": "2.2",
        "drupal/migrate_tools": "4.1",
        "drupal/redirect": "1.6",
        "drupal/redirect_after_login": "2.6",
        "drupal/redis": "1.4",
        "drupal/samlauth": "3.0-alpha2",
        "drupal/search_api": "1.18",
        "drupal/search_api_autocomplete": "1.4",
        "drupal/search_api_solr": "4.1.8",
        "drupal/token": "1.6",
        "drupal/pathauto": "1.6",
        "drupal/views_data_export": "1.0",
        "unb-libraries/drupal-builder": "^8.0.0",
        "unb-libraries/saml_features": "dev-8.x-1.x"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
