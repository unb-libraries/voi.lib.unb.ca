{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of voi.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "patches": {
      "drupal/flag_search_api": {
        "Multi-valued document index issue": "https://www.drupal.org/files/issues/2020-11-21/3183850.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/voi.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "3.3.0",
    "drupal/bootstrap4": "2.1.16",
    "drupal/core": "9.5.5",
    "drupal/drush_language": "1.0-rc5",
    "drupal/facets": "2.0.6",
    "drupal/flag": "4.0-beta4",
    "drupal/flag_search_api": "dev-1.x",
    "drupal/fontawesome": "2.25",
    "drupal/google_analytics": "2.5",
    "drupal/health_check": "1.3",
    "drupal/redirect": "1.8",
    "drupal/redirect_after_login": "2.7",
    "drupal/redis": "1.6",
    "drupal/samlauth": "3.8",
    "drupal/search_api": "1.28",
    "drupal/search_api_autocomplete": "1.7",
    "drupal/search_api_solr": "4.2.10",
    "drupal/views_data_export": "1.3",
    "unb-libraries/drupal-builder": "^9",
    "unb-libraries/saml_features": "dev-9.x-1.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^9"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ]
  },
  "type": "project"
}
